doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1.page-title Video Games
        table
            each video_game in data
                tr      
                    td #{video_game.properties.name} 
                    td #{video_game.properties.publisher}
                    td #{video_game.properties.price}

        div.button-wrapper
            a.button-new(href="/update-cobj") Add to this table




